{
  "name": "dydev-client",
  "version": "1.1",
  "title": "DYDEV",
  "description": "DYDEV Client",
  "author": {
    "name": "leedy",
    "url": ""
  },
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "41.4.2",
    "@ckeditor/ckeditor5-font": "^41.4.2",
    "@ckeditor/ckeditor5-react": "7.0.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/icons-material": "5.15.18",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "5.15.18",
    "@mui/styles": "5.15.18",
    "@mui/x-date-pickers": "6.19.5",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "apexcharts": "3.49.1",
    "axios": "1.6.7",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "history": "5.3.0",
    "html-react-parser": "^5.1.10",
    "notistack": "^3.0.1",
    "nprogress": "0.2.0",
    "numeral": "2.0.6",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-apexcharts": "1.4.1",
    "react-chartjs-2": "5.2.0",
    "react-custom-scrollbars-2": "4.5.0",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.2",
    "react-router": "6.23.1",
    "react-router-dom": "6.23.1",
    "react-scripts": "5.0.1",
    "stylis": "4.3.2",
    "stylis-plugin-rtl": "2.1.1",
    "web-vitals": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-prod": "dotenv -e .env.prod react-scripts build",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn install",
    "clean-win": "del /f /s /q node_modules\\ 1>nul && rmdir /s /q node_modules\\ && del yarn.lock && yarn install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --config ./.prettierrc",
    "dist": "yarn lint && yarn build",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@types/numeral": "2.0.5",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": ">=9",
    "lint-staged": ">=15",
    "node-sass": "9.0.0",
    "prettier": "3.2.5",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  }
}
